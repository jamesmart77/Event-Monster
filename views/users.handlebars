    <div id="searchPanel" class="col-md-12">
        <h1>Welcome to the Users List</h1>
        <br>
        <button type="submit" id="create-user">New User</button>
        <br>
        <br>
        <div class="panel panel-info">
            <div class="panel-heading">All Users</div>
            <div class="panel-body">
           
                <table class="table">
                    <thead>
                        <tr>
                            <th scope="col">First Name</th>
                            <th scope="col">Last Name</th>
                            <th scope="col">Email Address</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#each users}}
                        <tr>
                            <td>{{first_name}}</td>
                            <td>{{last_name}}</td>
                            <td>{{email}}</td>
                        </tr>
                        {{/each}}
                    </tbody>
                </table>


                <table>

                </table>

              <!-- </div> -->
            </div>
        </div>
    </div>
</div>

