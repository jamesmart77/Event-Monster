    <h1>Welcome to the Users Page</h1>
    <br>
    <a class="btn-floating btn-large waves-effect waves-light red" id="create-user"><i class="material-icons">add</i></a>
    <br>
    <br>
    <div class="row">
        <div class="col s10 offset-s1">
            <table id="users-table">
                <thead>
                    <tr>
                        <th scope="col">First Name</th>
                        <th scope="col">Last Name</th>
                        <th scope="col">Email Address</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each users}}
                    <tr>
                        <td>{{first_name}}</td>
                        <td>{{last_name}}</td>
                        <td>{{email}}</td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>